<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Button?>

<VBox alignment="CENTER" spacing="15.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ak.ui.TransactionHistoryController">
    <children>
        <TableView fx:id="transactionTable" prefWidth="600" prefHeight="400">
            <columns>
                <TableColumn text="Date" fx:id="dateColumn" prefWidth="200" />
                <TableColumn text="Type" fx:id="typeColumn" prefWidth="200" />
                <TableColumn text="Amount" fx:id="amountColumn" prefWidth="200" />
            </columns>
        </TableView>
        <Button text="Back to Account" onAction="#handleBackToAccount" />
    </children>
</VBox>